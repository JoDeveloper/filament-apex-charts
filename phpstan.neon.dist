includes:
    - phpstan-baseline.neon

parameters:
    level: 4
    paths:
        - src
        - config
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    checkMissingIterableValueType: false
    ignoreErrors:
    - '#Access to an undefined property Jodeveloper\\FilamentApexCharts\\Widgets\\ApexChartWidget::\$form.#'
